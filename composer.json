{
    "name": "mazarini/test",
    "description": "Base tools for test.",
    "type": "library",
    "minimum-stability": "dev",
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "docker": false
        },
        "public-dir": "tests/public",
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "symfony/console": "^6.3|^7.0",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^6.3|^7.0",
        "symfony/runtime": "^6.3|^7.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.17",
        "phpunit/phpunit": "^9.5",
        "symfony/debug-bundle": "^6.3|^7.0",
        "symfony/dotenv": "^6.3|^7.0",
        "symfony/maker-bundle": "^1.50",
        "symfony/phpunit-bridge": "^6.3|^7.0",
        "symfony/stopwatch": "^6.3|^7.0",
        "symfony/twig-bundle": "^6.3|^7.0",
        "symfony/yaml": "^6.3|^7.0"
    },
    "license": "GPL-3.0-or-later",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Mazarini\\Test\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mazarini\\Test\\Tests\\": "tests/tests",
            "App\\": "tests/src/"
        }
    },
    "replace": {},
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "authors": [
        {
            "name": "Mazarini",
            "email": "mazarini@protonmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "reference": "config"
            }
        }
    ]
}
